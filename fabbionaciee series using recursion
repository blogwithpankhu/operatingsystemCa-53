#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int fibbo(int n){
    if(n<=1)
      return n;
    else
      return fibbo(n-1)+fibbo(n-2);
}
int main(){
  int n,i=0;
  cout<<"Enter number of series of fibbonaciie series";
  cin>>n;
  while(i<n){
      cout<<fibbo(i)<<" ";
      i++;
  }
}
